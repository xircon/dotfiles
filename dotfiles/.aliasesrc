## emacs function:
e(){
    sudo="/sudo::"
    
    if [[ ${1:0:1} == "/" ]] ; then
        str=$sudo$1
    else
        str=$1
    fi

    if [[ ${1:0:5} == "/home" ]] ; then
    	str=$1
    fi

    if [ "$1" = "" ]; then
       #Open my notes file:
       emacsclient -c ~/.t.org &
    else
       #Open file from command line:
       emacsclient -c $str &
    fi
    echo "Opening: "$str
}

